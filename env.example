# Configuración para Vercel
# Copia este archivo como .env.local para desarrollo local

# API Key de Google Gemini (requerida para el Asistente IA)
VITE_GEMINI_API_KEY=tu_api_key_de_gemini_aqui

# Configuración de la aplicación
VITE_APP_NAME=Nutrigenómica y Genotipos
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# URLs de la aplicación
VITE_APP_URL=https://nutrigenomica-y-genotipos.vercel.app
VITE_API_URL=https://api.tudominio.com

# Configuración de analytics (opcional)
VITE_ANALYTICS_ID=tu_id_de_analytics

# Configuración de monitoreo (opcional)
VITE_SENTRY_DSN=tu_dsn_de_sentry
